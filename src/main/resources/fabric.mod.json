{
	"schemaVersion": 1,
	"id": "pehkui",
	"name": "Pehkui",
	"version": "${version}",
	"description": "Allows resizing of most entities.",
	"authors":
	[
		"Virtuoel"
	],
	"contributors":
	[
		"Draylar",
		"haykam821",
		"Leximon",
		"MerchantPug",
		"TheGrimsey"
	],
	"contact":
	{
		"homepage": "https://www.curseforge.com/minecraft/mc-mods/pehkui",
		"issues": "https://github.com/Virtuoel/Pehkui/issues",
		"sources": "https://github.com/Virtuoel/Pehkui"
	},
	"license": "MIT",
	"icon": "assets/pehkui/icon.png",
	"environment": "*",
	"depends":
	{
		"fabricloader": ">=0.14.24",
		"fabric-api-base": "*",
		"kanos_config": ">=0.4.1"
	},
	"entrypoints":
	{
		"main":
		[
			"virtuoel.pehkui.Pehkui"
		],
		"client":
		[
			"virtuoel.pehkui.PehkuiClient"
		]
	},
	"mixins":
	[
		"pehkui.mixins.json"
	],
	"custom":
	{
		"multiconnect":
		{
			"custom_payloads":
			{
				"allowed_clientbound": "pehkui:*"
			}
		}
	}
}
